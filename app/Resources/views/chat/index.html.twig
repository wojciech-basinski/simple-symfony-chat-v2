{% block content %}
    <div class="row chat" id="chat-row-main">

        <div class="col-xs-7 col-md-10" id="messages">
            <div class="panel panel-success" id="panel-messages">
                <div class="panel-body panel-messages no-padding">
                    <div id="messages-box">
                    </div>
                </div>
            </div>
        </div>
        <div class="col-xs-5 col-md-2" id="online-users">
            <div class="panel panel-success panel-no-padding" id="panel-users">
                <div class="panel-body no-padding">
                    <div class="text-bold info-text" role="button" data-toggle="collapse"
                         href="#collapseLanguage" aria-expanded="true">{{ 'lang'|trans }}:</div>
                    <div id="collapseLanguage" class="collapse">
                        <div class="text-in-info language {{ locale == 'pl' ? 'active' }}" data-value="pl">PL</div>
                        <div class="text-in-info language {{ locale == 'en' ? 'active' }}" data-value="en">EN</div>
                    </div>
                    <div class="text-bold info-text" role="button" data-toggle="collapse"
                         href="#channels" aria-expanded="true">{{ 'channels'|trans }}:</div>
                    <div id="channels" class="collapse in"></div>
                    <div class="text-bold info-text" role="button" data-toggle="collapse"
                         href="#users-box" aria-expanded="true">{{ 'online'|trans }}:</div>
                    <div id="users-box" class="collapse in">
                        <div class="{{ user.getChatRoleAsText }} text-in-info" id="username">{{ user.username }}</div>
                    </div>
                    <div class="logout">
                        {% if is_granted('ROLE_ADMIN') %}
                            <a href="{{ path('chat_admin') }}" target="_blank">{{ 'panel'|trans }}</a>
                        {% endif %}
                        <a href="{{ path("chat_logout") }}" class="btn btn-success">{{ 'logout'|trans }}</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-xs-10" id="emoticon-row">
            <div class="row">
                <div class="emoticons-div" name="emoticons"></div>
                <div id="roll"><img class="emoticon-text kursor" id="roll-img" src="{{ asset('img/kostka.png') }}"></div>
            </div>
        </div>
        <div class="col-xs-2" id="settings">
            <div class="row">
                <div class="ok" id="status"></div>
                <div id="sound" class="pointer"></div>
                <div id="scroll" class="pointer"></div>
            </div>
        </div>
        <div class="col-xs-12" id="bbcode-row">
            <div class="row">
                <div class="bbcode-div" id="bbcode">
                    <button class="btn btn-success text-bold" data-toggle="tooltip"
                            data-title="{{ 'bbcode.boldLabel'|trans }}"
                            data-bbcode="b">{{ 'bbcode.boldText'|trans }}</button>
                    <button class="btn btn-success text-italic" data-toggle="tooltip"
                            data-title="{{ 'bbcode.italicLabel'|trans }}"
                            data-bbcode="i">{{ 'bbcode.italicText'|trans }}</button>
                    <button class="btn btn-success text-underline" data-toggle="tooltip"
                            data-title="{{ 'bbcode.underlineLabel'|trans }}"
                            data-bbcode="u">{{ 'bbcode.underlineText'|trans }}</button>
                    <button class="btn btn-success" data-toggle="tooltip" data-title="{{ 'bbcode.quoteLabel'|trans }}"
                            data-bbcode="quote">{{ 'bbcode.quoteText'|trans }}</button>
                    <button class="btn btn-success" data-toggle="tooltip" data-title="{{ 'bbcode.codeLabel'|trans }}"
                            data-bbcode="code">{{ 'bbcode.codeText'|trans }}</button>
                    <button class="btn btn-success" data-toggle="tooltip" data-title="{{ 'bbcode.urlLabel'|trans }}"
                            data-bbcode="url">{{ 'bbcode.urlText'|trans }}</button>
                    <button class="btn btn-success" data-toggle="tooltip" data-title="{{ 'bbcode.imgLabel'|trans }}"
                            data-bbcode="img">{{ 'bbcode.imgText'|trans }}</button>
                </div>
            </div>
        </div>
        <div class="col-xs-12 margin-top-minus" id="text-row">
            <div class="row">
                <div id="emoji-container"></div>
                <div class="col-xs-10">
                    <textarea class="form-control" id="message-text" cols="50" rows="2"></textarea>
                    {#<div id="emoji-container"></div>#}
                </div>
                <div class="col-xs-2">
                    <button class="btn btn-success btn-block" id="send">{{ 'send'|trans }}</button>
                </div>
            </div>
        </div>
    </div>
    <div class="youtube-video">
        <span class="close">&times;</span>
        <iframe class="youtube-video-iframe"></iframe>
    </div>
    <div class="modal fade" tabindex="-1" role="dialog" id="modal-error">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">{{ 'error.errorOccured'|trans }}</h4>
                </div>
                <div class="modal-body">
                    <p id="modal-text"></p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
{% endblock %}
